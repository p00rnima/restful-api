{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "id",
    "name",
    "data"
  ],
  "properties": {
    "id": {
      "type": "string",
      "default": "",
      "title": "The id Schema",
      "examples": [
        "1"
      ]
    },
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": [
        "Google Pixel 6 Pro"
      ]
    },
    "data": {
      "type": "object",
      "default": {},
      "title": "The data Schema",
      "required": [
        "color",
        "capacity"
      ],
      "properties": {
        "color": {
          "type": "string",
          "default": "",
          "title": "The color Schema",
          "examples": [
            "Cloudy White"
          ]
        },
        "capacity": {
          "type": "string",
          "default": "",
          "title": "The capacity Schema",
          "examples": [
            "128 GB"
          ]
        }
      },
      "examples": [{
        "color": "Cloudy White",
        "capacity": "128 GB"
      }]
    }
  },
  "examples": [{
    "id": "1",
    "name": "Google Pixel 6 Pro",
    "data": {
      "color": "Cloudy White",
      "capacity": "128 GB"
    }
  }]
}